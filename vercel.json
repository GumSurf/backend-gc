{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/main.ts"
        }
    ],
    "functions": {
        "api/**/*.ts": {
            "maxDuration": 10
        }
    }
}